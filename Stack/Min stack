// problem : minstack
class MinStack {

Stack<Integer> minstack ;
Stack<Integer> maxstack ;
    public MinStack() {
        minstack = new Stack<>();
        maxstack = new Stack<>();
        
    }
    
    public void push(int val) {
        maxstack.push(val);
        if(minstack.isEmpty()||val<=minstack.peek()){
            minstack.push(val);
        }
        else {
            minstack.push(minstack.peek());
        }
    }
    
    public void pop() {
        maxstack.pop();
        minstack.pop();
        
    }
    
    public int top() {
        return maxstack.peek();
        
    }
    
    public int getMin() {
        return minstack.peek();
        
    }
}
