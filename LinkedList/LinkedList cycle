// problem : linkedlist cycle
public class Solution {
    public boolean hasCycle(ListNode head) {
        
        int count = 0 ;
        HashSet<ListNode> map = new HashSet<>();
        ListNode curr = head;

        while(curr!=null){
            if(map.contains(curr)){
                return true;
            }
            else {
                map.add(curr);
                curr=curr.next;
            }
        }
        return false;
        
    }
}
