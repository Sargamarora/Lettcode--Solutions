problem : sort list
     public ListNode sortList(ListNode head){
        if(head==null || head.next==null) return head;
        ListNode mid = middle(head);
        ListNode left = head;
        ListNode right = mid.next;
        mid.next=null;
        left = sortList(left);
        right =sortList(right);
        return merge(left,right);
     }
    private ListNode middle(ListNode head){
        ListNode slow = head;
        ListNode fast = head;
        ListNode prev = null;
        while(fast!=null && fast.next!=null){
            prev=slow;
            slow = slow.next;
            fast=fast.next.next;
        }
        return prev;
    }

    private ListNode merge(ListNode list1 , ListNode list2){
        ListNode dummy = new ListNode(-1);
        ListNode temp = dummy;
        while(list1!=null && list2!=null){
            if(list1.val<=list2.val){
                temp.next=list1;
                list1=list1.next;
            }
            else {
                temp.next=list2;
                list2=list2.next;
            }
            temp=temp.next;
        }
        while(list1!=null){
            temp.next=list1;
            list1=list1.next;
            temp=temp.next;
        }
        while(list2!=null){
            temp.next=list2;
            list2=list2.next;
            temp=temp.next;
        }
        return dummy.next;
    }

}
